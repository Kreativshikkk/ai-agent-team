<idea-plugin>
    <id>com.example.agentteam</id>
    <name>Agent Team Builder</name>
    <vendor email="you@example.com">Your Name</vendor>

    <description><![CDATA[
    Wizard для создания «команды агентов». По Submit полностью
    заменяет содержимое проекта выбранным ZIP-архивом.
  ]]></description>

    <depends>com.intellij.modules.platform</depends>

    <extensions defaultExtensionNs="com.intellij">
        <toolWindow id="Internies"
                    anchor="left"
                    factoryClass="com.example.agentteam.net.TeamBuilderToolWindowFactory"/>
    </extensions>

    <idea-version since-build="251" until-build="251.*"/>
</idea-plugin>
